{
  "meta": {
    "version": 2,
    "key": "fedclient.key"
  },
  "server": {
    "host": "localhost",
    "port": "443",
    "ssl": "on",
    "ssl_key_file": "_localhost.key",
    "ssl_cert_file": "_localhost.crt",
    "ssl_generate_keys": "on"
  },
  "preferences": {
    "jwt": {
      "library": "jwcrypto"
    },
    "logging": {
      "handler": [
        "file",
        "console",
        "web_console"
      ]
    },
    "open_webconsole": "on",
    "clipboard": {
      "encrypt_clipboard": "on",
      "remember_secrets": "off"
    }
  },
  "default": {
    "saml": {
      "idp_authentication_binding_capabilities": ["urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect", "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST"],
      "idp_logout_binding_capabilities": ["urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect", "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST"]
    }
  },
  "idps": {
  }
}
